{
    "env": {
		"node": true,
        "browser": true,
        "es2021": true
    },
    "extends": [
        "google",
        "plugin:@typescript-eslint/eslint-recommended",
        "plugin:@typescript-eslint/recommended"
    ],
    "overrides": [
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
      "require-jsdoc": 0,
      "new-cap": 0,
      "@typescript-eslint/no-this-alias": 0,
      "@typescript-eslint/ban-ts-comment": 1
    }
}
