{
  "compilerOptions": {
    "moduleResolution": "node",
    "skipLibCheck": true,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "sourceMap": true,
    "strictNullChecks": true,
    "lib": [
      "DOM",
      "es2020"
    ],
    "module": "es2020"
  },
  "exclude": [
    "./node_modules",
    "./dist",
    "./public"
  ]
}